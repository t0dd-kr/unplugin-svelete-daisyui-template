{
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "all",
	"semi": false,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"printWidth": 80,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"htmlWhitespaceSensitivity": "ignore",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
